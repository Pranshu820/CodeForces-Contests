#include <bits/stdc++.h>
#define ll long long
#define mod 1000000007
using namespace std;



int main(){
ios_base::sync_with_stdio(false); cin.tie(0);
int t; cin >> t; while(t--){
    string s;
    cin >> s;
    int e = count(s.begin(), s.end(), 'E');
    int n = count(s.begin(), s.end(), 'N');

    if(n >= 2 || !n || !e) cout << "YES\n";
    else cout << "NO\n";
    
}
    return 0;
}
