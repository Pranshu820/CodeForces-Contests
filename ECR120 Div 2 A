#include <bits/stdc++.h>
#define ll long long
#define mod 1000000007
using namespace std;



int main(){
ios_base::sync_with_stdio(false); cin.tie(0);
int t; cin >> t; while(t--){
    int a, b, c;
    cin >> a >> b >> c;

    if((a + b + c) & 1) cout << "NO\n";
    else{
        if(a + b == c || b + c == a || c + a == b) cout << "YES\n";
        else if(a == b || b == c || c == a) cout << "YES\n";
        else cout << "NO\n";
    }
}
    return 0;
}
