#include <bits/stdc++.h>
#define ll long long
#define mod 1000000007
using namespace std;



int main(){
ios_base::sync_with_stdio(false); cin.tie(0);
int t; cin >> t; while(t--){
    int n;
    cin >> n;
    for(int i = 2; n; i++){
        cout << i << " ";
        n--;
    }
    cout << "\n";
}
    return 0;
}
